{
    "authorEmail": "rrmerugu@gmail.com",
    "authorName": "Ravi",
    "authorWebsite": "rrmerugu.github.io",
    "config": {
        "dataPoints": {
            "linkScraper": [
                {
                    "name": "title",
                    "nthElement": "0",
                    "selector": "a.blog-title",
                    "valueSize": "string",
                    "valueType": "text"
                },
                {
                    "name": "content",
                    "nthElement": "0",
                    "selector": ".blog-post-body.markdown-body",
                    "valueSize": "string",
                    "valueType": "innerHTML"
                },
                {
                    "name": "date",
                    "nthElement": "0",
                    "selector": ".blog-post-meta li:first-child",
                    "valueSize": "string",
                    "valueType": "text"
                }
            ],
            "links": {
                "nthElement": "0",
                "selector": "h2.blog-post-title a",
                "valueSize": "array",
                "valueType": "href"
            },
            "pagination": {
                "doPagination": true,
                "nextButton": {
                    "selector": "a",
                    "contains": "Next"
                },
                "scrapeMaxSize": 1500,
                "scrapeMinSize": ""
            }
        },
        "method": "requests",
        "scrapeType": "list",
        "website": "https://github.com/blog"
    },
    "scraperName": "github.blog",
    "scraperVersion": "1"
}